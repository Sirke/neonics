summary(N9)
#tarkistetaan ettei globaalimalli ole ylidispersoitunut
E2 <- resid(N9, type = "pearson")
N  <- nrow(dataS)
p  <- length(fixef(N9)$con) + 3  #2 sigmas and 1 size/k/theta NB parameter
sum(E2^2) / (N - p)
